{
  "logPattern": "20",
  "rules": [{
    "name": "Connection state",
    "states": [{
      "name": "Unknown",
      "bgColor": "white",
      "paths": [{
	"to": "connected",
	"pattern": ["<bind xmlns=\\\"urn:ietf:params:xml:ns:xmpp-bind\\\">", "<auth-success><meta-token>"]
      }, {
	"to": "disconnected",
        "pattern": ["onSocket:willDisconnectWithError:", "TDTAppDelegate application:didFinishLaunchingWithOptions"]
      }, {
	"to": "connecting",
	"pattern": ["Sending connect to Socket", "reallyConnect"]
      }]
    }, {
      "name": "connected",
      "bgColor": "#d7fdc7",
      "paths": [{
	"to": "disconnected",
	"pattern": ["onSocket:willDisconnectWithError:", "TDTAppDelegate application:didFinishLaunchingWithOptions"]
      }]
    },{
      "name": "connecting",
      "bgColor": "#ffe5c6",
      "paths": [{
        "to": "connected",
	"pattern": ["<bind xmlns=\\\"urn:ietf:params:xml:ns:xmpp-bind\\\"", "<auth-success><meta-token>"]
      }, {
	"to": "disconnected",
	"pattern": ["onSocket:willDisconnectWithError:", "TDTAppDelegate application:didFinishLaunchingWithOptions"]
      }]
    }, {
      "name": "disconnected",
      "bgColor": "#ffc5c3",
      "paths": [{
	"to": "connecting",
	"pattern": ["Sending connect to Socket", "reallyConnect"]
      }]
    }]
  }, {
    "name": "App state",
    "states": [{
      "name": "Unknown",
      "bgColor": "white",
      "paths": [{
	"to": "active",
	"pattern": ["Did Become Active"]
      }, {
	"to": "background",
	"pattern": ["Did Enter Background"]
      }]
    }, {
      "name": "active",
      "bgColor": "#fff2bf",
      "paths": [{
	"to": "background",
	"pattern": ["Did Enter Background"]
      }]
    }, {
      "name": "background",
      "bgColor": "#d2d2d2",
      "paths": [{
	"to": "active",
	"pattern": ["Did Become Active"]
      }]
    }]
  }]
}
